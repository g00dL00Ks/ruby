<h1>Listing pros</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pros.each do |pro| %>
      <tr>
        <td><%= pro.name %></td>
        <td><%= pro.user_id %></td>
        <td><%= pro.user %></td>
        <td><%= link_to 'Show', pro %></td>
        <td><%= link_to 'Edit', edit_pro_path(pro) %></td>
        <td><%= link_to 'Destroy', pro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pro', new_pro_path %>
